extends /layouts/application.pug

block content
  +jumb
    +formFor(form, errors, { url: urlFor('sessionsCreate'), method: 'post' })
      +input(form, errors, 'email', { label: 'Email' })
      +input(form, errors, 'password', { label: 'Password', as: 'password' })
      +button(form, { type: 'submit', value: 'Log in', class: '' })
